{
  "name": "eatster_landing",
  "version": 2,
  "alias": ["eatster.app"],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/", "dest": "dist/index.html" },
    { "src": "/vop", "dest": "vop.html" },
    { "src": "/2020", "status": 301, "headers": { "Location": "https://forms.gle/R2nxshHRUd2pwppc6" } }
  ]
}
